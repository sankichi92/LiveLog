create_table :entries, force: :cascade do |t|
  t.references :song, null: false, index: { unique: true }
  t.references :member, null: false
  t.text :notes
  t.text :admin_memo

  t.timestamps

  t.foreign_key :songs
  t.foreign_key :members
end

create_table :playable_times, force: :cascade do |t|
  t.references :entry, null: false
  t.tsrange :range, null: false

  t.timestamps

  t.index :range, using: :gist
  t.foreign_key :entries
end
